ExposureGraphNodes 
| where NodeLabel == @"user"
| where parse_json(NodeProperties).rawData.primaryProvider == "ActiveDirectory"
| where parse_json(NodeProperties).rawData.userAccountControl[2] == 'PasswordNeverExpires'
| extend PWUpdateTime = todatetime((NodeProperties).rawData.passwordUpdateTime)
| where PWUpdateTime < ago(365d)
